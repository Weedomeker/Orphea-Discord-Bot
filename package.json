{
  "name": "Bot",
  "version": "0.1.0",
  "description": "Discord Bot ",
  "main": "main.js",
  "repository": "https://github.com/Weedomeker/bot-discord.git",
  "author": "Weedomeker <weedomeker@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "github:discordjs/discord.js",
    "emoji-unicode": "^2.0.0",
    "node-cron": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "eslint-config-aqua": "^7.2.4"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off"
    }
  }
}
